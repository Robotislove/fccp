<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find clean_car_description)/config/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find clean_car_description)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find clean_car_description)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find clean_car_description)/config/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find clean_car_description)/config/teb_local_planner_params.yaml" command="load" />
    <rosparam file="$(find clean_car_description)/config/move_base_params.yaml" command="load" />
    
    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
    <remap from="/move_base/cmd_vel" to="/cmd_vel" />
  </node>
  
</launch>
